.points-head-box{
  width:95%;
  height:100rpx;
  line-height:100rpx;
  margin:0px auto;
  margin-top:50rpx;
  margin-bottom:50rpx;
  display: flex;
  flex-direction: row;
}
.points-img{
  width:104rpx;
  height:104rpx;
}
.points-img image{
  width:100%;
  height:100%;
}
.points-num {
  font-size: 80rpx;
  margin-left:30rpx;
  font-weight:bold;
}

.points-text {
  font-size: 40rpx;
  margin-left:10rpx;
}
.points-center-box{
  width:450rpx;
  height:80rpx;
}
.points-rule{
  min-width:50rpx;
  height:45rpx;
  line-height:45px;
  font-size:32rpx;
  color:#a2a8ba;
}
.points-items{
  width:95%;
  height:130rpx;
  margin:0px auto;
  margin-top:30rpx;
  display: flex;
  flex-direction: row;
  border-bottom:1rpx solid #45485b;
}
.points-date-detail{
  width:50%;
}
.points-date-detail .points-date{
  color:#a2a8ba;
}
.points-date-detail text{
  display:block;
  font-size:32rpx;
  height:60rpx;
  line-height:60rpx;
}
.points-num-detail{
  width:50rpx;
  height:130rpx;
  line-height: 130rpx;
  flex: 1;
  text-align: right;
}
